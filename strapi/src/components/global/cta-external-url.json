{
  "collectionName": "components_global_cta_external_urls",
  "info": {
    "displayName": "CTA external URL",
    "icon": "cursor"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "external_url": {
      "type": "string",
      "regex": "^(https?:\\/\\/)?([a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})(:[0-9]{1,5})?(\\/[^\\s?#]*)?(\\?[^\\s#]*)?(#[^\\s]*)?$",
      "default": "http://example.com"
    }
  }
}
