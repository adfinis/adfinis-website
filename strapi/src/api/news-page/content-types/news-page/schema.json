{
  "kind": "collectionType",
  "collectionName": "news_pages",
  "info": {
    "singularName": "news-page",
    "pluralName": "news-pages",
    "displayName": "Page News",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "metadata_title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "metadata_description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "metadata_title",
      "required": true
    },
    "hero": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::hero.hero"
    },
    "card_title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "card_subtitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "minutes_read": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true,
      "default": 1
    },
    "main_blog": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "sections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.heading-with-link-container",
        "sections.feature-cards",
        "sections.quote-section",
        "sections.content-carousel",
        "sections.color-card-carousel",
        "sections.cta-banner",
        "sections.single-column-section",
        "sections.regular-form-section",
        "sections.image-carousel",
        "sections.blog-content-section"
      ]
    }
  }
}
